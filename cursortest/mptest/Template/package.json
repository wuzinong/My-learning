{
  "name": "poseidon-app",
  "description": "Poseidon client appliction",
  "version": "1.0.0",
  "author": "DNVGL",
  "homepage": ".",
  "private": true,
  "scripts": {
    "clean": "rimraf ./build",
    "start": "react-app-rewired start",
    "build": "rimraf ./build && react-app-rewired build",
    "build:MVP": "rimraf ./build && cross-env REACT_APP_SECRET_ENV='MVP' react-app-rewired build",
    "build:view": "rimraf ./build && ANALYZER=true react-app-rewired build",
    "test": "jest",
    "git": "git pull && git add . && git commit -m",
    "postgit": "git push",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "precommit": "lint-staged",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.3",
    "@veracity/js-internal": "3.0.300143-beta",
    "antd": "^4.0.4",
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.23",
    "http-proxy-middleware": "^1.0.4",
    "mockjs": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-intl": "^4.2.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.2",
    "react-svg": "^11.0.15",
    "rimraf": "^3.0.2",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.3",
    "@types/mockjs": "^1.0.2",
    "@types/node": "^12.0.0",
    "@types/prismjs": "^1.16.1",
    "@types/react": "^16.9.25",
    "@types/react-csv": "^1.1.1",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "antd-dayjs-webpack-plugin": "^0.0.9",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^4.0.0",
    "conventional-changelog-cli": "^2.0.34",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "customize-cra": "^0.9.1",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.2",
    "react-app-rewired": "^2.1.5",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.5",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testRegex": "/__test__/.*/.*\\.tsx",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testPathIgnorePatterns": [
      "/(node_modules|lib|coverage|types)/"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
